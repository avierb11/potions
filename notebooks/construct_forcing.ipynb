def f(x: float) -> float:
    k: float = 0.1 
    alpha: float = 2.484
    base: float = max(0, x)
    return -k * base ** alpha 

def midpoint(x: float) -> float:
    x_0: float = 25.0
    dt: float = 1
    return (x - x_0) - dt * f(0.5 * (x_0 + x))

def rk2(x_0: float) -> float:
    dt: float = 1.0
    return x_0 + dt * f(x_0 + 0.5 * dt * f(x_0))

def sln(x_0: float, k: float, alpha: float, ts: NDArray) -> NDArray:
    return (x_0 ** (1 - alpha) - (1 - alpha) * k * ts) ** (1 / (1 - alpha))
